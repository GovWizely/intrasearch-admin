<%= @environment %>:
  action_mailer:
    default_url_options:
      :protocol: https
      :host: '<%= @action_mailer_url_host %>'
      :port: <%= @action_mailer_url_port %>
    smtp_settings:
      :address: '<%= @smtp_address %>'
      :user_name: '<%= @smtp_user_name %>'
      :password: '<%= @smtp_password %>'
